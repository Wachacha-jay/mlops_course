{
  "expectation_suite_name": "feature_expectation_suite",
  "meta": {
    "description": "Feature-level contracts for Walmart sales forecasting (Week 3)",
    "great_expectations_version": "0.18.0"
  },
  "expectations": [
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": { "column": "weekly_sales" }
    },

    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "weekly_sales",
        "min_value": 0
      }
    },

    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "week_of_year",
        "min_value": 1,
        "max_value": 53
      }
    },

    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "year",
        "min_value": 2000,
        "max_value": 2030
      }
    },

    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "sales_lag_1",
        "min_value": 0
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "sales_lag_2",
        "min_value": 0
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "sales_lag_4",
        "min_value": 0
      }
    },

    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "sales_roll_mean_4",
        "min_value": 0
      }
    },

    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "is_holiday"
      }
    }
  ]
}
