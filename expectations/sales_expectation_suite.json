{
  "expectation_suite_name": "sales_expectation_suite",
  "meta": {
    "great_expectations_version": "1.10.0",
    "description": "Validation rules for raw Walmart weekly sales ingestion (adjusted to current CSV columns)"
  },
  "expectations": [
    {
      "expectation_type": "expect_table_columns_to_match_ordered_list",
      "kwargs": {
        "column_list": [
          "Store",
          "Dept",
          "Date",
          "Weekly_Sales",
          "IsHoliday"
        ]
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "Date"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_strftime_format",
      "kwargs": {
        "column": "Date",
        "strftime_format": "%Y-%m-%d"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "Store",
        "min_value": 1,
        "max_value": 1000
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "Dept",
        "min_value": 1,
        "max_value": 1000
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "Weekly_Sales",
        "min_value": -100000
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "IsHoliday",
        "value_set": [true, false]
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "Store"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "Dept"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "Weekly_Sales"
      }
    }
  ]
}
